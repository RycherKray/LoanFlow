{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    // Starts Kestrel with both http & https, no IIS Express
    "LoanFlow.Api": {
      "commandName": "Project",
      "dotnetRunMessages": true, // shows the listening URLs in console
      "launchBrowser": true, // opens default browser
      "applicationUrl": "https://localhost:7151;http://localhost:5151",
      "launchUrl": "api/health",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}